        "@babel/preset-react": "^7.18.6",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.5",
        "bootstrap": "^5.2.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^7.3.0",
